@model SiteManagementProject.Models.User
@{
    ViewData["Title"] = "GetLogin";

    Layout = "~/Views/Shared/_LoginLayout.cshtml";
}


<div class="container-fluid">
    <div class="row">
        <div class="col-md-8">


            <table class="table table-hover table-sm">
                <tr>
                    <th>İsim:</th>
                    <td>@Model</td>
                </tr>
                <tr>
                    <th>Soyisim:</th>
                    <td>@Model.SurName</td>
                </tr>
                <tr>
                    <th>Email:</th>
                    <td>@Model.Email</td>
                </tr>
                <tr>
                    <th>Telefon:</th>
                    <td>@Model.PhoneNumber</td>
                </tr>
                @*<tr>
                        <th>Resim:</th>
                        <td>@Model.PictureURL</td>
                    </tr>*@

            </table>

        </div>

    </div>
</div>



